//求100~200间的全部素数。
//在前者基础之上

int main()
{
	int n,k,i,m=0;
	for (n=101;n<=200;n=n+2) //n=n+2,在奇数中再进行判断
	{
		k=sqrt(n);
		for (i=2;i<=k;i++)
			if (n%i==0)
				break;
			if (i>=k)
			{
				printf("%d\t",n);
				m=m+1; //只是换行作用
			}
			if (m%10==0) //每十个一换行
				printf("\n");
	}
	printf("\n");
}

